{% import "_helpers.j2" as helpers %}
# Managed by ansible.  Do not modify

[Match]
{% if cfg.match.path is defined %}
Path={{ cfg.match.path }}
{% endif %}
{% if cfg.match.macaddress is defined %}
MACAddress={{ cfg.match.macaddress }}
{% endif %}

[Link]
{% if cfg.link.name %}
Name={{ cfg.link.name }}
{% endif %}
{% if cfg.link.tso is defined %}
TCPSegmentationOffload={{ helpers.bool(cfg.link.tso) }}
{% endif %}
{% if cfg.link.gso is defined %}
GenericSegmentationOffload={{ helpers.bool(cfg.link.gso) }}
{% endif %}
{% if cfg.link.tso6 is defined %}
TCP6SegmentationOffload={{ helpers.bool(cfg.link.tso6) }}
{% endif %}
